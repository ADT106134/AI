    "BMI = 0\n",
    "height = int(input(\"請輸入身高(CM): \"))\n",
    "weight = int(input(\"請輸入體重(KG): \"))\n",
    "BMI = weight / ((height / 100) * (height / 100))\n",
    "print(\"你的BMI為: %f\" % (BMI))\n",
    "if BMI < 18.5:\n",
    "    print(\"你過瘦了!\")\n",
    "elif BMI >= 24:\n",
    "    print(\"你過胖了!\")\n",
    "else:\n",
    "    print(\"真健康!\")"
